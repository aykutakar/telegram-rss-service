{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "api/index.js",
      "methods": ["GET", "POST"]
    }
  ],
  "env": {
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token",
    "WEBHOOK_SECRET": "@webhook_secret"
  }
}
